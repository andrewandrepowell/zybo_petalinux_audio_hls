#ifndef FIR_CPPWRAP_H_
#define FIR_CPPWRAP_H_

#include "fir.h"

class fir_cppwrap
{
public:
	fir_cppwrap() { }
	void start( uint32_t phy_base_addr, void* param )
	{
		this->param = param;
	}
private:
	fir obj;
	void* param;
};

#endif /* FIR_CPPWRAP_H_ */
